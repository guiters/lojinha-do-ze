{
  "drive": "DriveMysql",
  "database": "lojinhadoze",
  "connections": "local",
  "table": "orders",
  "columns": [
    "id",
    "user_id",
    "product_id",
    "timestamp"
  ],
  "limitpage": "100",
  "where": {
    "optional": {
      "id": "/^[a-zA-Z0-9]+$/",
