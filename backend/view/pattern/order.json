      "product_id": "/^[a-zA-Z0-9]+$/",
      "timestamp": "/^[a-zA-Z0-9]+$/"
    }
  },
  "requires": {
    "REQUEST": [
      "GET",
      "POST",
      "PUT",
      "DELETE"
    ]
  }
}